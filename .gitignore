# ============ ENVIRONMENT & SECRETS ============
# Environment files (ALL subdirectories)
.env
.env.*
*/.env
*/.env.*
web/.env
web/.env.*
server/.env
server/.env.*
android/.env
android/.env.*

# API Keys and Secrets
*.key
*.pem
*.p12
*.keystore
secrets/
config/secrets/

# ============ NODE.JS / WEB ============
# Dependencies
node_modules/
*/node_modules/
web/node_modules/
server/node_modules/
scripts/node_modules/

# Build outputs
dist/
dist-ssr/
build/
*/dist/
*/build/
web/dist/
web/build/
server/dist/
server/build/

# Logs
logs/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*
lerna-debug.log*
web/logs/
server/logs/

# Runtime data
pids/
*.pid
*.seed
*.pid.lock

# Coverage directory used by tools like istanbul
coverage/
*.lcov

# Package manager files
package-lock.json
yarn.lock
pnpm-lock.yaml

# ============ ANDROID ============
# Built application files
*.apk
*.ap_
*.aab

# Files for the ART/Dalvik VM
*.dex

# Java class files
*.class

# Generated files
bin/
gen/
out/

# Gradle files
.gradle/
android/.gradle/
android/build/
android/*/build/

# Local configuration file (sdk path, etc)
local.properties
android/local.properties

# Proguard folder generated by Eclipse
proguard/
proguard-project.txt

# Log Files
android/*.log

# Android Studio Navigation editor temp files
.navigation/

# Android Studio captures folder
captures/

# IntelliJ
*.iml
.idea/
android/.idea/
android/*.iml

# Keystore files
*.jks
*.keystore

# External native build folder generated in Android Studio 2.2 and later
.externalNativeBuild/
.cxx/

# Google Services (e.g. APIs or Firebase)
google-services.json

# Freeline
freeline.py
freeline/
freeline_project_description.json

# fastlane
fastlane/report.xml
fastlane/Preview.html
fastlane/screenshots
fastlane/test_output
fastlane/readme.md

